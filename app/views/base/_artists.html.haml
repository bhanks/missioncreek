.front.artists.span8
	%ul
		- @artists.each do |artist|
			%li=link_to artist.name, '#'
	#artistCarousel.carousel.slide
		.carousel-inner
			.item
				%image{'src' => 'https://s3.amazonaws.com/missioncreek-assets/static/the-englert.jpeg'}
